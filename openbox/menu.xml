<menu id="apps-accessories-menu" label="Accessories">
</menu>

<menu id="virtual-menu" label="Virtual">
  <item label="VirtualBox">
    <action name="Execute">
      <command>VirtualBox</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
  <item label="Steam Win">
    <action name="Execute">
      <command>win_steam</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
  <item label="Flawless Win">
    <action name="Execute">
      <command>win_flawless</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
</menu>

