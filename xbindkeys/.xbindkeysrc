
# Examples of commands:

"xbindkeys_show" 
  control+shift + q

# set directly keycode (here control + f with my keyboard)
"xterm"
  c:41 + m:0x4

# specify a mouse button
"xterm"
  control + b:2

# increase by 3%
"amixer -q sset Master 3%+"
  XF86AudioRaiseVolume

# decrease by 3%
"amixer -q sset Master 3%-"
  XF86AudioLowerVolume

# mute/unmute
#"amixer -q sset Master toggle"
"toggle_mute"
  XF86AudioMute

"xbacklight -inc 10"
  XF86MonBrightnessUp

"xbacklight -dec 10"
  XF86MonBrightnessDown

# Lock this shit
"lock"
  control+p

"toggle"
  XF86Tools

